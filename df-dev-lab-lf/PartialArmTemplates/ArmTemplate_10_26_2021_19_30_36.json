{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-dev-lab-lf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ls_akv_devlabls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "https://kvdevlabfs.vault.azure.net/"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_sqldb_devlabls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": {
						"type": "SecureString",
						"value": "**********"
					},
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "ls_akv_devlabls",
							"type": "LinkedServiceReference"
						},
						"secretName": "secsqldb01"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_akv_devlabls')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/sqldbdevlablf_metas_csv')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_sqldb_devlabls",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "sa_sqldb_devlablf"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "loja",
						"type": "varchar"
					},
					{
						"name": "competencia",
						"type": "varchar"
					},
					{
						"name": "meta",
						"type": "varchar"
					},
					{
						"name": "data_ini",
						"type": "varchar"
					},
					{
						"name": "data_fim",
						"type": "varchar"
					},
					{
						"name": "d01",
						"type": "varchar"
					},
					{
						"name": "d02",
						"type": "varchar"
					},
					{
						"name": "d03",
						"type": "varchar"
					},
					{
						"name": "d04",
						"type": "varchar"
					},
					{
						"name": "d05",
						"type": "varchar"
					},
					{
						"name": "d06",
						"type": "varchar"
					},
					{
						"name": "d07",
						"type": "varchar"
					},
					{
						"name": "d08",
						"type": "varchar"
					},
					{
						"name": "d09",
						"type": "varchar"
					},
					{
						"name": "d10",
						"type": "varchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "stg_metas_csv"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_sqldb_devlabls')]"
			]
		}
	]
}