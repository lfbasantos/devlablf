			"defaultValue": "df-dev-lab-lf"
		},
		"ls_sa_devlab_lf_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sa_devlab_lf'"
		},
		"ls_sqldb_devlabls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_devlabls'"
		},
		"ls_akv_devlabls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvdevlabfs.vault.azure.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
