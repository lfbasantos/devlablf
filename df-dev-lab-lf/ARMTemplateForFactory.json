				"[concat(variables('factoryId'), '/linkedServices/ls_sa_devlab_lf')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_akv_devlabls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
